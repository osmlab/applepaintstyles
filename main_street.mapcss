  casing-opacity: 0.3; 
  casing-dashes: 3,3; 
  text: eval(cond( tag("name"), concat("Name(w) = ", tag("name"), " & ", "Ref(w) = ", tag("ref")," | ","Name(r) = ", parent_tag("name"), " & ", "Ref(r) = ", parent_tag("ref")),concat("Name = ", parent_tag("name"), " | ", "Ref = ", parent_tag("ref")) )); 
