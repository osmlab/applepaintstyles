    text-offset: 40;
}

area[amenity=embassy][setting("sensitive")],
area[office=diplomatic][setting("sensitive")],
area[diplomatic=~/embassy|consulate/][setting("sensitive")] {
    text:tr("DIPLOMATIC BUILDING");
    text-position: center;
    font-size: 20;
    color: #FFA500;
    width: 3;
}

